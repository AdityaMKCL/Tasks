<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.app.models">
	<class name="ExamEventPaperDetails" table="EXAMEVENTPAPERDETAILS">
		<id name="examEventPaperID" column="EXAMEVENTPAPERID">
			<generator class="identity" />
		</id>
		<property name="examEventID" column="EXAMEVENTID" />
		<property name="fkPaperID" column="FKPAPERID" />
		<property name="noOfAttempts" column="NOOFATTEMPTS" />
		<property name="isRandomOption" column="ISRANDOMOPTION" />
		<property name="allowAttemptType" column="ALLOWATTEMPTTYPE" />
		<property name="createdBy" column="CREATEDBY" />
		<property name="dateModified" column="DATEMODIFIED" type="timestamp"/>
		<property name="dateCreated" column="DATECREATED" type="timestamp"/>
		<property name="modifiedBy" column="MODIFIEDBY" />
		<property name="deleted" column="DELETED" />
		<property name="isIdenticalItems" column="ISIDENTICALITEMS" type="boolean" />
		<property name="isIdenticalItemSequence" column="ISIDENTICALITEMSEQUENCE" type="boolean" />
		
		<property name="isSkipQuestion" column="ISSKIPQUESTION" type="boolean" />
		<property name="isAnswerChange" column="ISANSWERCHANGE" type="boolean" />
		<property name="showNotepad" column="SHOWNOTEPAD" type="boolean" />
		<property name="palletNavigation" column="PALLETNAVIGATION" type="boolean" />

	</class>
</hibernate-mapping>
