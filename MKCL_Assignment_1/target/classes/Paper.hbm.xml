<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.app.models">
	<class name="Paper" table="PAPER">
		<id name="paperID" column="PAPERID">
			<generator class="identity" />
		</id>
		<property name="fkClientID" column="FKCLIENTID" />
		<property name="code" column="CODE" />
		<property name="name" column="NAME" />
		<property name="duration" column="DURATION" />
		<property name="isPublished" column="ISPUBLISHED" />
		
		<property name="dateCreated" column="DATECREATED" />
		<property name="createdBy" column="CREATEDBY" />
		<property name="dateModified" column="DATEMODIFIED"/>
		<property name="modifiedBy" column="MODIFIEDBY" />
		<property name="deleted" column="DELETED" />
		<property name="Total_Marks_Check" column="TOTALMARKS" />

	</class>
</hibernate-mapping>
